<ribbon xmlns="http://github.com/SolidEdgeCommunity/SolidEdge/Ribbon">
  <tab name="SyN-BooM">
	  <group name="Guardar">

		  <button id="10"
			  size="large"
			  label="Guardar"
			  screentip="Guardar"
			  supertip="--"
			  imageId="10"
			  enabled="true" />

	  </group>
	  
	  <group name="Infor-XA">
      <button id="2"
             size="large"
             label="BooM  Infor-XA"
             screentip="Reporte Infor-XA"
             supertip="Exporta el reporte de ocurrencias del conjunto actual.&#13;&#13;Por defecto:&#13;&#13;*No incluye hijos de elementos soldados.&#13;*No incluye ocurrencias excluidas.&#13;*No incluye Subconjuntos.&#13;*Incluye las cantidades necesarias para su fabricación."
             imageId="2"
             enabled="true" />
		
		<checkBox id="1"
			 label="Incluir Hijos"
			 screentip="Incluir Hijos"
			 supertip="Cuando está activado, configura 'BooM Infor-XA' para incluir en el reporte, los hijos de elementos soldados.&#13;&#13;Sólo es válido para el próximo uso. Después se desactivará"/>
	  
     </group>

	  <group name="Utillaje">
		  
		  <button id="7"
			  size="large"
			  label="Relacion  de Utillaje"
			  screentip="Exportar utillaje"
			  supertip="Exporta la RELACION DE UTILLAJE del conjunto actual, en la plantilla seleccionada.&#13;&#13;Por defecto:&#13;&#13;*Incluye hijos de elementos soldados.&#13;*No incluye ocurrencias excluidas.&#13;*No incluye Subconjuntos.&#13;*Incluye las cantidades necesarias para su fabricación."
			  imageId="7"
			  enabled="true" />

		  <!--<checkBox id="4"
				  label="Incluir rutas"
				  screentip="Incluir Ruta."
				  supertip="Incluir Ruta." />

		  <checkBox id="3"
				  label="Exportar Utillaje"
				  screentip="Exportar Utillaje."
				  supertip="Exportar Utillaje ." />-->
		  
	  </group>

