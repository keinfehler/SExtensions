<ribbon xmlns="http://github.com/SolidEdgeCommunity/SolidEdge/Ribbon">
  <tab name="Helpers">
    <group name="Ocurrencias">
      <button id="2"
             size="large"
             label="Utillaje"
             screentip="Exportar a excel las ocurrencias"
             supertip="Exportar a excel las ocurrencias"
             imageId="2"
             enabled="true" />
     </group>

	  <group name="Opciones">
		  <checkBox id="1"
			  label="Incluir Hijos.pwd"
			  screentip="Incluir Hijos.pwda"
			  supertip="Incluir Hijos .pwd"/>

		  <checkBox id="4"
				  label="Incluir rutas"
				  screentip="Incluir Ruta."
				  supertip="Incluir Ruta." />

		  <checkBox id="3"
				  label="Exportar Utillaje"
				  screentip="Exportar Utillaje."
				  supertip="Exportar Utillaje ." />
		  
	  </group>

    <group name="Incluir / Excluir Ocurrencias">
      
      <button id="5"
              size="large"
              label="Incluir"
              screentip="Incluir ocurrencias"
              supertip="Incluir ocurrencias"
              imageId="117"
              enabled="true" />
      
      <button id="6"
              size="large"
              label="Excluir"
              screentip="Excluir ocurrencias"
              supertip="Excluir ocurrencias"
              imageId="118"
              enabled="true" />
    </group>
  </tab>
</ribbon>